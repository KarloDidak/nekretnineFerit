<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".NekretnineFeritApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_house"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_house"
        android:supportsRtl="true"
        android:theme="@style/Theme.NekretnineFerit"
        tools:targetApi="31">
        <activity
            android:name=".activities.SigningActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.BrowsingActivity"
            android:exported="false" />
    </application>
</manifest>